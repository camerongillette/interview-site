<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/cam-interview/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="cam-interview/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cam-interview/components/container"
            editable="{Boolean}true"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cam-interview/components/container"
                editable="{Boolean}true"
                layout="responsiveGrid">
                <headerexperiencefragment
                    jcr:createdBy="admin"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cam-interview/components/experiencefragment"/>
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cam-interview/components/title"
                    editable="{Boolean}true"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cam-interview/components/container"
                    editable="{Boolean}true"
                    layout="responsiveGrid"/>
                <footerexperiencefragment
                    jcr:createdBy="admin"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cam-interview/components/experiencefragment"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>
